# Required
GOOGLE_API_KEY=your-gemini-api-key

# Search backends (at least one recommended)
SERPER_API_KEY=your-serper-key      # serper.dev - $50/mo
EXA_API_KEY=your-exa-key            # exa.ai - semantic search

# Optional
AGENCY_MODEL=gemini-2.0-flash       # Override default model
AGENCY_LIVE_TOOLS=1                 # Enable live API calls (default: stubs)
AGENCY_SEARCH_BACKEND=serper        # serper | exa (default: serper)
